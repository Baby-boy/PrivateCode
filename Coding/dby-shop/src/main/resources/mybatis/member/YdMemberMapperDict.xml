<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd.dby.c.member.mapper.YdMemberMapperDict">

	<select id="listByCode" resultType="java.util.HashMap">
		SELECT
			dict_id, dt_code, dict_value
		FROM
			yd_dict
		WHERE
			dt_code = #{dt_code}
		ORDER BY
			dict_sort ASC
	</select>
	
</mapper>