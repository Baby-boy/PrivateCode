<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd.dby.b.shop.mapper.YdShopMapperSetting">
	
	<select id="info" resultType="com.yd.dby.b.shop.entity.YdShopSetting">
		SELECT
			name, value
		FROM
			yd_setting
		WHERE
			name = #{name}
	</select>
	    
</mapper>