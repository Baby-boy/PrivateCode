<div class="row" ng-controller="topStationListCtrl">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <!-- <div class="ibox-title">
                <h5>{{title}}</h5>
            </div> -->
            <div class="ibox-content">
                <form class="form-inline">
	               <div class="row m-t">
						<div class="form-group col-xs-3">
							<label for="subTime">伙伴名称:</label>
							<input type="text" class="form-control" ng-model="para.news_title" placeholder="请输入关键字" >
						</div>
						<div class="col-md-1">
							<button type="button" class="btn btn-primary" ng-click="getNewsList()">立即搜索</button>
						</div>
						<div class="col-md-2">
							<button type="button" class="btn btn-primary" ng-click="getNewsList()">添加合作伙伴</button>
						</div>
					</div>
					<div class="row m-t">
						
					</div>
				  </form>
		 	  <div >
 		       <table class="table table-striped table-bordered table-hover dataTables-example">
                      <tr>
                            <th style="text-align: center;">序号</th>
                            <th style="text-align: center;">标题</th>
                            <th style="text-align: center;">简称</th>
                            <th style="text-align: center;">创建单位</th>
                            <th style="text-align: center;">创建时间</th>
                            <th style="text-align: center;">流失装填</th>
                            <th style="text-align: center;">状态</th>
                            <th style="text-align: center;">操纵</th>
                        </tr>
                        <tr ng-repeat="station in topStationList">
                            <td>{{station.stationId}}</td>
                            <td>{{station.stationTitle}}</td>
                            <td>{{station.stationSimple}}</td>
                            <td>{{station.stationCompany}}</td>
                            <td>{{station.createTime | date:'yyyy-MM-dd HH:mm'}}</td>
                            <td>{{station.lossState}}</td>
                            <td ng-if="station.lossState == 1" ng-style="station.lossState == 1?{color:'gray'}:''">在园区</td>
                            <td ng-if="station.lossState == 2" ng-style="station.lossState == 2?{color:'blue'}:''">淘汰</td>
                            <td ng-if="station.lossState == 3" ng-style="station.lossState == 3?{color:'red'}:''">流失</td>
                            <td>{{station.stationState}}</td>
                            <td ng-if="station.stationState == 1" ng-style="station.stationState == 1?{color:'gray'}:''">已上传</td>
                            <td ng-if="station.stationState == 2" ng-style="station.stationState == 2?{color:'blue'}:''">已发布</td>
                            <td ng-if="station.stationState == 3" ng-style="station.stationState == 3?{color:'red'}:''">已删除</td>
                            <td><a href="###">详情</a>  | <a href="###">修改</a>  | <a href="###">取消置顶</a></td>
                         </tr>
            	   </table>
                    <!--分页开始-->
					   <ndtech-pagination conf="paginationConf"></ndtech-pagination>
					<!--分页结束-->
 		       </div>
            </div>
        </div>
    </div>
    
</div>

