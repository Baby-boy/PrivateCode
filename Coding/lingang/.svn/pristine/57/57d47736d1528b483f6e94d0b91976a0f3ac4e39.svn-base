'use strict';

define(["serviceBase"], function (serviceBase) {
	 var items = {};
	    //获取管理员列表
	    items.sysPolicyPageList = function ($http,data) {
	        var request = $http.post('policy/queryAllByPage.do', data, serviceBase.postConfig);
	        return( request.then(serviceBase.handleSuccess, serviceBase.handleError) );
	    };

    return ['sysPolicyService', items];
});