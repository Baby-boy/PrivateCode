<mapper namespace="com.lingang.core.persistence.reader.SysBasicsReaderMapper" >
  <resultMap id="BaseResultMap" type="com.lingang.api.domain.entity.SysBasics" >
    <id column="basics_id" property="basicsId" jdbcType="INTEGER" />
