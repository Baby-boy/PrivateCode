<div class="row" ng-controller="sysParkPageListCtrl">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>{{title}}</h5>
                <a ng-class="nav == 1?'btn-primary':'btn-info'" class="btn btn-sm" href="javascript:void(0)">批量删除产业园区</a>
                <a ng-class="nav == 1?'btn-primary':'btn-info'" class="btn btn-sm" href="javascript:void(0)">添加产业园区</a>
            </div>
            <div class="ibox-content">
                <form class="form-inline">
	               <div class="row m-t">
						<div class="form-group col-xs-3">
							<label for="subTime">园区名称:</label>
							<input type="text" class="form-control" ng-model="para.parkNameKeywords" placeholder="请输入产业园区名称.." >
						</div>
						<div class="form-group col-xs-3">
							<label for="subTime">园区地址:</label>
							<input type="text" class="form-control" ng-model="para.parkAddrKeywords" placeholder="请输入产业园区地址.." >
						</div>
						<!-- <div class="form-group col-xs-3">
							<label for="subTime">下单时间:</label>
							<input type="text" id="subTime" class="form-control" ng-model="orderTime" mindate="1900-10-10 00:00:00" date-picker placeholder="请选择时间..." >
						</div> -->
						<div class="col-md-3">
							<button type="button" class="btn btn-primary" ng-click="getParkList()">立即搜索</button>
						</div>
					</div>
					<div class="row m-t">
						
					</div>
				  </form>
		 	  <div >
 		       <table class="table table-striped table-bordered table-hover dataTables-example">
                      <tr>
                            <th style="text-align: center;">序号</th>
                            <th style="text-align: center;">名称</th>
                            <th style="text-align: center;">地区名称</th>
                            <th style="text-align: center;">详细地址</th>
                            <th style="text-align: center;">创建时间</th>
                            <th style="text-align: center;">更新时间</th>
                            <th style="text-align: center;">详细内容</th>
                            <th style="text-align: center;">状态</th>
                            <th style="text-align: center;">操作</th>
                        </tr>
                        <tr ng-repeat="park in parkList">
                            <td>{{$index + 1}}<input type="hidden" name="parkId" value="{{park.parkId}}"/></td>
                            <td>{{park.parkName}}</td>
                            <td>{{park.regionName}}</td>
                            <td>{{park.parkAddress}}</td>
                            <td>{{park.createTime | date:'yyyy-MM-dd HH:mm'}}</td>
                            <td>{{park.updateTime | date:'yyyy-MM-dd HH:mm'}}</td>
                            <td><a href="javascript:void(0)">详情</a></td>
                            <td>
                               <span ng-if="park.parkState == 1" ng-style="park.parkState == 1?{color:'gray'}:''">已上传</span>
                               <span ng-if="park.parkState == 2" ng-style="park.parkState == 2?{color:'blue'}:''">已发布</span>
                            </td>
                            <td>
                            	<a href="javascript:void(0)">修改</a>
                            	<a href="javascript:void(0)">删除</a>
                            </td>
                        </tr>
            	   </table>
                   <div class="row m-t">
					  <div class="col-md-10 col-md-offset-1">
					  		<!--分页开始-->
							<ndtech-pagination class conf="paginationConf"></ndtech-pagination>
							<!--分页结束-->
					  </div>
				   </div>
 		       </div>
            </div>
        </div>
    </div>
    
</div>

