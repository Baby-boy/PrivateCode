<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd.gcj.mapper.YdMangerMapperAwards">
	<select id="$queryAll" parameterType="java.lang.Integer" resultType="com.yd.gcj.entity.YdMangerAwards">
		select 
			awards_id,awards_caseid,awards_name,awards_desc,awards_create_time 
		from 
			yd_awards
		where
			awards_caseid = #{awards_caseid}
	</select>
	<select id="$queryById" parameterType="java.lang.Integer" resultType="com.yd.gcj.entity.YdMangerAwards">
		select 
			awards_id,awards_caseid,awards_name,awards_desc,awards_create_time
		from 
			yd_awards
		where 
			awards_id = #{awards_id}
	</select>
	
	<select id="$queryCountNum" parameterType="java.lang.Integer" resultType="java.lang.Integer">
		select 
			count(*)
		from 
			yd_awards
		where 
			awards_caseid = #{awards_caseid}
	</select>
	
	<insert id="$insert" parameterType="com.yd.gcj.entity.YdMangerAwards" useGeneratedKeys="true" keyProperty="awards_id">
		insert into yd_awards
			(awards_caseid,awards_name,awards_desc,awards_create_time)
		value
			(#{awards_caseid},#{awards_name},#{awards_desc},#{awards_create_time})
	</insert>
	
	<update id="$update" parameterType="com.yd.gcj.entity.YdMangerAwards">
		update yd_awards
		<set>
			<trim suffixOverrides=",">
				<if test="awards_caseid != null">awards_caseid = #{awards_caseid},</if>
				<if test="awards_name != null">awards_name = #{awards_name},</if>
				<if test="awards_desc != null">awards_desc = #{awards_desc},</if>
				<if test="awards_create_time != null">awards_create_time = #{awards_create_time},</if>
			</trim>
		</set>
		where awards_id = #{awards_id}
	</update>
	
	<delete id="$delete" parameterType="java.lang.Integer">
		delete form yd_awards
		where awards_id = #{awards_id}
	</delete>
	
</mapper>

