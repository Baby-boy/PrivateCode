<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd.gcj.system.mapper.YdMangerMapperSystemUserLabel">
	<!-- 后台 -->
	<select id="queryAllServerSkillState" resultType="com.yd.gcj.entity.vo.YdMangerUserLabelVo" >
		SELECT yul.*,yu.* 
		FROM yd_user_label yul 
		LEFT JOIN yd_user yu ON yul.userl_uid=yu.user_id
 		where userl_uid=#{user_id}
	</select>
	
</mapper>