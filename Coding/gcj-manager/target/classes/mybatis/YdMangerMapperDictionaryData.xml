<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd.gcj.mapper.YdMangerMapperDictionaryData">
	
	<select id="$queryAllByValue" resultType="com.yd.gcj.entity.YdMangerDictionaryData">
		select * 
		from 
			yd_dictionarydata
		where 
			dict_value = #{value}
	</select>
	
	<select id="$queryCountByValue" resultType="java.lang.Integer">
		select
			count(*)
		from 
			yd_dictionarydata
		where
			dict_value = #{value}
	</select>
	
	<select id="$queryAllByValueAndIsfixed" resultType="com.yd.gcj.entity.YdMangerDictionaryData">
		select * 
		from 
			yd_dictionarydata
		where 
			dict_value = #{value} and isfixed = #{isfixed}
	</select>
	
	<select id="$queryCountByValueAndIsfixed" resultType = "java.lang.Integer">
		select count(*)
		from 
			yd_dictionarydata
		where 
			dict_value = #{value} and isfixed = #{isfixed}
	</select>
	
	<select id="$queryById" resultType="com.yd.gcj.entity.YdMangerDictionaryData">
		select 
			*
		from 
			yd_dictionarydata
		where
			dict_id = #{id}
	</select>
	
	<select id="$isExsitById">
		select count(*) 
		from 
			yd_dictionarydata
		where 
			dict_id = #{id}
	</select>
	
</mapper>

