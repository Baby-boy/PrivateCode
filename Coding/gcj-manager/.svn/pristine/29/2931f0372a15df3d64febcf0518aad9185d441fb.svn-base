.sub-nav{
  width: 100%;
  height: 130px;
  background-color: #f7f7f7;
  .sub-nav-wrap{
    width: 1200px;
    height: 100%;
    margin: 0 auto;
    border-left: 1px solid #eeeeee;
    border-right: 1px solid #eeeeee;
    >a{
      display: block;
      float: left;
      height: 100%;
      &.icon_bag{
        background: url('../images/icon_swing.png') 21px 94px no-repeat,url('../images/icon_bag.png') 150px 28px no-repeat;
      }
      &.icon_task{
        background: url('../images/icon_swing.png') 21px 94px no-repeat,url('../images/icon_task.png') 163px 29px no-repeat;
      }
      &.icon_workhall{
        background: url('../images/icon_swing.png') 21px 94px no-repeat,url('../images/icon_workhall.png') 155px 31px no-repeat;
      }
      &.icon_tanlent{
        background: url('../images/icon_swing.png') 21px 94px no-repeat,url('../images/icon_tanlent.png') 156px 28px no-repeat;
      }
      &.icon_call{
        background: url('../images/icon_swing.png') 21px 94px no-repeat,url('../images/icon_call.png') 160px 22px no-repeat;
      }
      &.bordered{
        border-left: 1px solid #eeeeee;
        border-right: 1px solid #eeeeee;
      }
      &:hover{
        dt{
          color: #3cc8b4;
        }
      }
    }
    dl{
      display: block;
      width: 240px;
      height: 100%;
      padding: 31px 0 0 20px;
      &.w-237{
        width: 237px !important;
      }
      dt{
        line-height: 1;
        margin-bottom: 6px;
        font-size: 18px;
        color: #333;
        font-weight: 700;
        -webkit-transition: color .2s ease;
        -moz-transition: color .2s ease;
        -ms-transition: color .2s ease;
        -o-transition: color .2s ease;
        transition: color .2s ease;
      }
      dd{
        font-size: 12px;
        color: #999;
      }
    }
  }
}

.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes slideInUp {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 20%, 0);
    transform: translate3d(0, 20%, 0);
    visibility: visible;
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@keyframes slideInUp {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 20%, 0);
    transform: translate3d(0, 20%, 0);
    visibility: visible;
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

.slideInUp {
  -webkit-animation-name: slideInUp;
  animation-name: slideInUp;
}
/*header*/
.header{
  width: 100%;
  height: 88px;
  overflow: hidden;
  background-color: #fff;
  .header-wrapper{
    width: 1200px;
    height: 100%;
    margin: 0 auto;
    .clearfix;
    .logo{
      position: relative;
      float: left;
      width: 222px;
      height: 100%;
      >img{
        display: block;
        &:first-child{
          position: absolute;
          top: 16px;
          left: 18px;
        }
        &:last-child{
          position: absolute;
          bottom: 21px;
          left: 0;
          width: 222px;
        }
      }
    }
    .nav-links{
      height: 100%;
      float: left;
      padding-left: 81px;
      >li{
        height: 100%;
        float: left;
        border-bottom: 2px solid transparent;
        &:hover,&.active{
          border-color: #3cc8b4;
          a{
            color: #3cc8b4;
          }
        }
        &:not(:last-child){
          margin-right: 29px;
        }
        a{
          display: block;
          height: 100%;
          padding-top: 50px;
          color: #666666;
          font-size: 14px;
        }
      }
    }
    .header-btns{
      float: right;
      height: 100%;
      padding-top: 42px;
      font-size: 0;
      -webkit-text-size-adjust: none;
      >li{
        display: inline-block;
        width: 64px;
        height: 24px;
        -webkit-border-radius: 12px;
        -moz-border-radius: 12px;
        border-radius: 12px;
        background-color: #cccccc;
        -webkit-transition: all .15s;
        -moz-transition:all .15s;
        -ms-transition: all .15s;
        -o-transition: all .15s;
        transition: all .15s;
        &:first-child{
          margin-right: 10px;
        }
        &.active{
          background-color: #3cc8b4;
        }
        a{
          display: block;
          height: 100%;
          line-height: 24px;
          text-align: center;
          width: 100%;
          color: #ffffff;
          font-size: 12px;
          font-family: "Microsoft YaHei";
        }
      }
    }
  }
}