/*reset*/
*,*:before,*:after{
  box-sizing: border-box;
}
html,body{
  position: relative;
  width: 100%;
  height: 100%;
}
body,ul,p,h1,h2,h3,h4,h5,h6,dl,dt,dd{
  margin: 0;
  padding: 0;
}
ol,ul{
  list-style: none;
}
.clearfix{
  &:before,&:after{
    content:' ';
    display: table;
  }
  &:after{
    clear:both;
  }
}
a{
  text-decoration: none;
}
/*banner*/
.banner{
  position: relative;
  width: 100%;
  height: 500px;
  overflow: hidden;
  background-color: #fff;
}
.item{
  display: none;
  position: absolute;
  top: 0;
  left:0;
  width: 100%;
  height: 100%;
  background:center 128px no-repeat;
  cursor: pointer;
  perspective: 1000px;
  transform-style: preserve-3d;
  h1{
    font-size: 24px;
    line-height: 1;
  }
  >.banner1-bg{
    position: absolute;
    max-width: 100%;
    top:128px;
    left:0;
  }
  .container{
    position: relative;
    width: 1200px;
    height: 100%;
    margin: 0 auto;
    //background-color: lightblue;
    .banner-left{
      margin-top: 136px;
      float: left;
    }
    .banner-right{
      float: right;
      padding-top: 80px;
      padding-right: 150px;
      -webkit-transform-style: preserve-3d;
      transform-style: preserve-3d;
      backface-visibility: hidden;
      trasform:translateZ(0);
    }
    .roll-stage{
      position: absolute;
      bottom: 0;
      right: 74px;
      width: 517px;
      height: 469px;
      perspective: 1000px;
      .layer{
        -webkit-transform-style: preserve-3d;
        transform-style: preserve-3d;
        backface-visibility: hidden;
        &.top-left,&.top-right,&.bottom-left,&.bottom-right{
          position: absolute;
        }
        &.top-left{
          top:79px;
          left:-13px;
          transform: translateZ(-30px);
        }
        &.top-right{
          top:61px;
          right: -63px;
          transform: translateZ(-30px);
        }
        &.bottom-left{
          bottom: 86px;
          left: 0;
          transform: translateZ(50px);
        }
        &.bottom-right{
          bottom: 87px;
          right: -100px;
          transform: translateZ(-29x);
        }
        >img{
          display: block;
        }
      }
    }
    .banner-text{
      >img {
        display: block;
      }
      .banner-text1{
        margin-bottom: 9px;
      }
      .banner-text2{
        margin-bottom: 30px;
      }
    }
    .banner-btn{
      display: inline-block;
      width: 144px;
      height: 60px;
      background: url('../images/banner1_btn.png') no-repeat;
      transition: all .15s cubic-bezier(.28,.58,.83,.67);
      &:hover{
        background-image: url('../images/banner1_btn_hover.png');
      }
    }
    .lines{
      position: absolute;
      top:243px;
      left: 464px;
      width: 116px;
      height: 121px;
      background-repeat: no-repeat;
      &.line-one{
        background: url('../images/banner1_lines.png') center center no-repeat;
      }
      &.line-two{
        background: url('../images/banner2_lines.png') left top no-repeat;
        top:224px;
        left:503px;
      }
    }
    .compu-img{
      position: absolute;
      top: 50%;
      left: 50%;
      margin-left: -625/2*1px;
      margin-top: -339/2*1px;
      img{
        display: block;
      }
    }
  }
}
.indicators{
  position: absolute;
  bottom: 22px;
  left:50%;
  z-index: 999;
  transform: translateX(-50%);
  li{
    float: left;
    width: 10px;
    height: 10px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    background-color: #3cc8b4;
    transition: all .15s cubic-bezier(.28,.58,.83,.67);
    cursor: pointer;
    &:not(:last-child){
      margin-right: 9px;
    }
    &.active{
      width: 36px;
    }
  }
}
.green{
  color: #3cc8b4;
}
.mb-47{
