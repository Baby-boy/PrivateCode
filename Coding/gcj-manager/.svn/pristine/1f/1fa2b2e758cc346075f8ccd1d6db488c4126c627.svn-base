package com.yd.gcj.entity;

import java.util.Date;

public class YdMangerFiles {

	/**编号（主键）*/
	private Integer files_id;
	/**文件名*/
	private String files_name;
	/**文件路径*/
	private String files_path;
	/**文件描述*/
	private String files_desc;
	/**文件大小（以KB为单位）*/
	private long files_size;
	/**文件大小（以KB为单位）*/
	private Date files_create_time;
	/**更新时间*/
	private Date files_update_time;
	
	/**
	 * 获取 编号（主键）
	 * @return files_id
	 */
	public Integer getFiles_id() {
		return files_id;
	}
	
	/**
	 * 设置 编号（主键）
	 * @param files_id
	 */
	public void setFiles_id(Integer files_id) {
		this.files_id = files_id;
	}
	
	/**
	 * 获取 文件名
	 * @return files_name
	 */
	public String getFiles_name() {
		return files_name;
	}
	
	/**
	 * 设置 文件名
	 * @param files_name
	 */
	public void setFiles_name(String files_name) {
		this.files_name = files_name;
	}
	
	/**
	 * 获取 文件路径
	 * @return files_path
	 */
	public String getFiles_path() {
		return files_path;
	}
	
	/**
	 * 设置 文件路径
	 * @param files_path
	 */
	public void setFiles_path(String files_path) {
		this.files_path = files_path;
	}
	
	/**
	 * 获取 文件描述
	 * @return files_desc
	 */
	public String getFiles_desc() {
		return files_desc;
	}
	
	/**
	 * 设置 文件描述
	 * @param files_desc
	 */
	public void setFiles_desc(String files_desc) {
		this.files_desc = files_desc;
	}
	
	/**
	 * 获取 文件大小（以KB为单位）
	 * @return files_size
	 */
	public long getFiles_size() {
		return files_size;
	}
	
	/**
	 * 设置 文件大小（以KB为单位）
	 * @param files_size
	 */
	public void setFiles_size(long files_size) {
		this.files_size = files_size;
	}
	

	/**
	 * 获取 文件大小（以KB为单位）
	 * @return files_create_time
	 */
	public Date getFiles_create_time() {
		return files_create_time;
	}
	
	/**
	 * 设置 文件大小（以KB为单位）
	 * @param files_create_time
	 */
	public void setFiles_create_time(Date files_create_time) {
		this.files_create_time = files_create_time;
	}
	
	/**
	 * 获取 更新时间
	 * @return files_update_time
	 */
	public Date getFiles_update_time() {
		return files_update_time;
	}
	
	/**
	 * 设置 更新时间
	 * @param files_update_time
	 */
	public void setFiles_update_time(Date files_update_time) {
		this.files_update_time = files_update_time;
	}
	
	
}
