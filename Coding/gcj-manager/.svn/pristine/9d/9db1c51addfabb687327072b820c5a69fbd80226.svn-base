.yd-detail-text{
	overflow: hidden;
	background: #fff;
	padding:16px 30px;
}
.yd-detail-text1{
	padding:14px 0 20px 0;
	border-bottom:1px solid #eee;
	color:#999;
	font-size: 14px;
}
.yd-detail-text2{
	margin-top:30px;
	overflow: hidden;
}
.yd-wrap-h2{
	border-bottom:1px solid #eee;
	padding:0 0 14px 0;
	overflow: hidden;
}
.yd-detail-text2 h2{
	color:#333;
	font-size: 14px;
	font-weight: normal;
	border-left: 2px solid #3cc8b4;
	float: left;
	padding-left:5px;
}
.yd-mindetail{
	overflow: hidden;
	border-bottom:1px solid #eee;
	padding-bottom:15px;
}
.yd-mindetail h5,.yd-mindetail2 h5{
	color:#333;
	font-size: 14px;
	font-weight: normal;
	padding:20px 0;
}
.yd-mindetail p,.yd-mindetail2 p{
	color:#999;
	font-size: 14px;
	line-height: 25px;
}
.yd-mindetail2{
	padding:10px 0 20px;
	border-bottom:1px solid #eee;
	overflow: hidden;
}
.yd-plan{
	padding:30px 0 20px;
	border-bottom:1px solid #eee;
	overflow:hidden; 
}
.yd-plan li{
	float: left;
	width: 246px;
}
.yd-plan li:first-child{
}
.yd-plan li strong{
	font-weight: normal;
	font-size: 16px;
	color:#333;
}
.yd-plan li strong i{
	font-size: 14px;
	color:#333;
	display: inline-block;
}
.yd-plan li:first-child strong{
	color:#ff9801;
	font-size: 24px;
}
.yd-plan li:first-child strong i{
	color:#ff9801;
	font-size:14px;
}
.yd-plan li:first-child span{
	margin-top: 10px;
}
.yd-plan li span{
	font-size: 14px;
	color:#666;
	display: block;
	margin-top:16px;
}
.yd-inform{
	overflow: hidden;
	margin-top: 30px;
}
.yd-inform1{
	overflow: hidden;
	border-bottom:1px solid #eee;
	padding-bottom:13px;
}
.yd-attachment{
	overflow: hidden;
	padding:20px 0;
}
.yd-attachment-text{
	overflow: hidden;
	padding: 0 0 20px;
	border-bottom:1px solid #eee;
}
.yd-attachment h5{
	font-size: 14px;
	color:#333;
	font-weight:normal;
}
.yd-remark-text{
	margin-top: 15px;
	overflow: hidden;
}
.yd-attachment a{
	color:#999;
	font-size: 14px;
	display: block;
	margin-top:15px;
	overflow: hidden;
	line-height: 26px;
}
.yd-attachment-text a i{
	display: inline-block;
	width: 26px;
	height: 23px;
	background: url(../images/work/img.png) no-repeat 0 -67px;
	margin:0 18px 0 0;
	float: left;
}
.yd-remark{
	margin-top: 30px;
}
.yd-remark h5{
	color:#333;
	font-size: 14px;
}
.yd-remark-text{
	overflow: hidden;
}
.yd-remark-text p{
	color:#999;
	font-size: 14px;
	line-height: 25px;
}

/*申请企业*/
.yd-detail-text .yd-apply{
	border-top:1px dashed #eee;
	padding:20px 0;
	overflow: hidden;
}
.yd-detail-text .yd-apply:first-child{
	border-top:0;
}
.yd-building-left{
	float: left;
}
.yd-building-right{
	float: right;
	color:#b1b1b1;
	font-size: 14px;
}
.yd-building-img{
	width: 84px;
	height: 84px;
	margin-right: 15px;
	float: left;
}
.yd-building-img img{
	width: 100%;
	height: 100%;
}
.yd-building-content{
	float: left;
	overflow: hidden;
}
.yd-building-content span{
	color:#333;
	font-size: 18px;
	display: block;
	margin-bottom:18px;
}
.yd-building-content span i{
	width: 14px;
	height: 12px;
	display: inline-block;
	background: url(../images/work/v.png) no-repeat;
	margin-left:5px;
}
.yd-building-content strong{
	color:#b1b1b1;
	font-size: 14px;
	font-weight: normal;
}
.yd-building-content strong i{
	width: 18px;
	height: 16px;
	display: inline-block;
	margin-right: 5px;
	background: url(../images/work/p1.png) no-repeat;
}
/*任务互动*/
.yd-interact{
	overflow: hidden;
	padding:0 0 15px 0;
	background: #fff;
}
.yd-interact textarea{
	width: 1115px;
	height: 146px;
	border:1px solid #eaeaea;
	background: #fafafa;
	resize:none;
	outline: none;
	margin:20px 30px 10px 30px;
	padding:12px;
}
.yd-interact span{
	float: right;
	font-size: 12px;
	color:#999;
	width:1134px;
	text-align: right;
	margin:0 30px 7px;
}
.yd-interact2{
	overflow: hidden;
	margin-top:10px;
	background: #fff;
	padding:20px;
	width: 1160px;
}
.yd-interact2-title{
	border-bottom:1px solid #eee;
}
.yd-interact2-title h2{
	color:#666;
	font-size: 16px;
	font-weight: normal;
	padding-bottom: 13px;
}
.yd-interact2-good{
	overflow: hidden;
}
.yd-interact2-good li{
	padding:15px 0;
	border-top:1px solid #eee;
	overflow: hidden;
}
.yd-interact2-good li:first-child{
	border-top: 0;
}
.yd-good-img{
	width: 64px;
	height: 64px;
	float: left;
	margin-right: 10px;
}
.yd-good-img img{
	width: 100%;
	height: 100%;
}
.yd-good-text{
	overflow: hidden;
	float: left;
	width: 93%;
}
.yd-good-head{
	overflow: hidden;
}
.yd-author{
	color:#2ab4a0;
	float: left;
	font-size: 12px;
}
.yd-time{
	float: right;
	color:#999;
	font-size: 12px;
}
.yd-reply{
	color:#666;
	font-size: 14px;
	margin:7px 0;
	display: block;
}
.yd-good-compay{
	overflow: hidden;
}
.yd-scheme{
	color:#999;
	font-size: 12px;
	/*float: left;*/
	display: block;
}
.yd-good-plan{
	color:#3cc8b4;
	font-size: 12px;
	float: right;
	cursor: pointer;
}
.yd-detail-mintext.yd-detail-text{
	background: none;
	padding:0;
}
.yd-good-replan{
	margin-top:10px;
	width: 100%;
	overflow: hidden;
}
.yd-good-replan textarea{
	width: 1048px;
	height: 70px;
	border:1px solid #eaeaea;
	background: #f7f7f7;
	padding:10px 15px;
	resize: none;
	margin-left: 75px;
	outline: none;
}
.yd-good-replan span{
	width: 100%;
	float: right;
	text-align: right;
	color:#999;
	font-size: 12px;
	overflow: hidden;
	margin-bottom:10px;
}
.yd-good-btn{
	overflow: hidden;
	width: 100%;
}
.yd-good-left{
	overflow: hidden;
}
.yd-good-comment{
	width: 1046px;
	height: 88px;
	border:1px solid #eaeaea;
	padding:13px 15px;
	margin-top: 16px;
	background: #f7f7f7;
	overflow: hidden;
	position: relative;
	margin-bottom:13px;
}
.yd-min-comment{
	overflow: hidden;
	position: absolute;
	top:30px;
	left:15px;
}
.yd-min-comment strong{
	color:#2ab4a0;
	font-size: 12px;
	display: block;
	margin-bottom: 13px;
	font-weight: normal;
}
.yd-min-comment strong i{
	font-style: normal;
	color:#b1b1b1;
	font-size: 12px;
}
.yd-min-comment p{
	font-size: 12px;
	color:#999;
}