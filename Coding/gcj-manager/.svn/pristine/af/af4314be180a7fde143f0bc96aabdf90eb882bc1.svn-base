<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <meta charset="utf-8" name="renderer" content="webkit" />
    <title>功城记</title>
    <link rel="stylesheet" href="/css/public.css"/>
    <link rel="stylesheet" href="/css/index.css"/>
</head>
<body th:with="user=${session.user},msgSize=${session.msgSize}" th:inline="text">
<!--顶部导航-->
<div th:include="public/hander"></div>

<!--待签合同-->
<div class="hetong-box">
    <div class="hetong">
        <!-- 服务商个人信息头部 -->
        <div th:include="public/fuwushang/hander"></div>
        <div class="hetong-xq clear">
            <div class="left">
                <a href="fuwushang">我的任务</a>
                <a href="wodezhanghu">我的账户</a>
                <a href="wodejianli">我的简历</a>
                <a href="woderenzheng-shiming">我的认证</a>
                <a href="wodepingjia">我的评价</a>
                <a href="wodeshoucang" class="now">我的收藏</a>
                <a href="wodexiaoxi-huifu" class="pos-relative">我的消息<span class="badge badge-my-msg" th:if="${msgSize!=null and msgSize.countMsgSize > 0}" th:text="${msgSize.countMsgSize}">66</span></a>
                <a href="tuikuanzhong">我的退款</a>
            </div>
            <div class="right">
                <div class="topr2">
                    <i></i>我的收藏
                </div>
                <ul class="myshoucang">
                    
                    <li th:each="collection,niter:${collections}" th:class="'colle'+${collection.colle_taskid}">
                        <div class="toplis">
                            <div class="titlel"><a th:href="'/task/task-detail/'+${collection.colle_taskid}">[[${collection.colle_tpn}]]</a></div>
                            <div class="titler">
                               	距结束：[[${collection.remainDateTime}]]
                            </div>
                        </div>
                        <div class="botm clear">
                            <div class="ones">
                                <p><i></i>地点：<span>[[${collection.colle_tpa}]]</span></p>
                                <p><i class="cha"></i>工期：<span>[[${collection.colle_term}]]</span></p>
                            </div>
                            <div class="ones">
                                <p class="dolor">[[${collection.colle_price}]]￥</p>
                                <div><i></i>[[${#dates.format(collection.colle_start_time,'yyyy年MM月dd日')}]]发布</div>
                            </div>
                            <div class="twos">
                                <a href="###" th:onclick="'clearCollect('+${collection.colle_taskid}+')'">取消收藏</a>
                            </div>
                        </div>
                    </li>
                    
                </ul>
            </div>
        </div>
    </div>
</div>

<!--底部-->
<div th:include="public/foot"></div>

</body>
<script src="/js/jquery-1.11.1.min.js"></script>
<script type="text/javascript" src="/js/chajian/jqkuozhan.js"></script>
<script type="text/javascript" src="/js/bg/ajaxPost.js"></script>
<script type="text/javascript" src="/js/bg/user.js"></script>
</html>