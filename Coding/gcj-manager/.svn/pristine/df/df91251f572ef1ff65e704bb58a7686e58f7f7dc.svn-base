package com.yd.gcj.entity;

import java.util.Date;

public class YdMangerEpcs {

	/**编号（主键）*/
	private Integer epcs_id;
	/**评价等级（0好评、1中评、2差评）*/
	private Integer epcs_evaluation;
	/**差评原因*/
	private String epcs_reason;
	/**评价内容*/
    private String epcs_contents;
	/**雇主id*/
	private Integer epcs_eid;
	/**雇主昵称*/
	private String epcs_ename;
	/**任务id*/
    private Integer epcs_tid;
    /**任务名称*/
    private String epcs_tname;
    /**工期（期限）*/
    private String epcs_term;
    /**信息添加时间*/
    private Date epcs_create_time;
    /**最后一次更新时间*/
    private Date epcs_update_time;
    
    /**好评标识描述*/
    private String evaluation;
    
	/**
	 * 获取 编号（主键）
	 * @return epcs_id
	 */
	public Integer getEpcs_id() {
		return epcs_id;
	}
	
	/**
	 * 获取 评价等级（0好评、1中评、2差评）
	 * @return epcs_evaluation
	 */
	public Integer getEpcs_evaluation() {
		return epcs_evaluation;
	}
	
	/**
	 * 设置 评价等级（0好评、1中评、2差评）
	 * @param epcs_evaluation
	 */
	public void setEpcs_evaluation(Integer epcs_evaluation) {
		this.epcs_evaluation = epcs_evaluation;
	}
	
	/**
	 * 获取 差评原因
	 * @return epcs_reason
	 */
	public String getEpcs_reason() {
		return epcs_reason;
	}

	/**
	 * 设置 差评原因
	 * @param epcs_reason
	 */
	public void setEpcs_reason(String epcs_reason) {
		this.epcs_reason = epcs_reason;
	}

	/**
	 * 设置 编号（主键）
	 * @param epcs_id
	 */
	public void setEpcs_id(Integer epcs_id) {
		this.epcs_id = epcs_id;
	}
	
	/**
	 * 获取 雇主id
	 * @return epcs_eid
	 */
	public Integer getEpcs_eid() {
		return epcs_eid;
	}
	
	/**
	 * 设置 雇主id
	 * @param epcs_eid
	 */
	public void setEpcs_eid(Integer epcs_eid) {
		this.epcs_eid = epcs_eid;
	}
	
	/**
	 * 获取 雇主昵称
	 * @return epcs_ename
	 */
	public String getEpcs_ename() {
		return epcs_ename;
	}
	
	/**
	 * 设置 雇主昵称
	 * @param epcs_ename
	 */
	public void setEpcs_ename(String epcs_ename) {
		this.epcs_ename = epcs_ename;
	}
	
	/**
	 * 获取 任务id
	 * @return epcs_tid
	 */
	public Integer getEpcs_tid() {
		return epcs_tid;
	}
	
	/**
	 * 设置 任务id
	 * @param epcs_tid
	 */
	public void setEpcs_tid(Integer epcs_tid) {
		this.epcs_tid = epcs_tid;
	}
	
	/**
	 * 获取 任务名称
	 * @return epcs_tname
	 */
	public String getEpcs_tname() {
		return epcs_tname;
	}
	
	/**
	 * 设置 任务名称
	 * @param epcs_tname
	 */
	public void setEpcs_tname(String epcs_tname) {
		this.epcs_tname = epcs_tname;
	}
	
	/**
	 * 获取 工期（期限）
	 * @return epcs_term
	 */
	public String getEpcs_term() {
		return epcs_term;
	}
	
	/**
	 * 设置 工期（期限）
	 * @param epcs_term
	 */
	public void setEpcs_term(String epcs_term) {
		this.epcs_term = epcs_term;
	}
	
	/**
	 * 获取 评价内容
	 * @return epcs_contents
	 */
	public String getEpcs_contents() {
		return epcs_contents;
	}
	
	/**
	 * 设置 评价内容
	 * @param epcs_contents
	 */
	public void setEpcs_contents(String epcs_contents) {
		this.epcs_contents = epcs_contents;
	}
	
	/**
	 * 获取 信息添加时间
	 * @return epcs_create_time
	 */
	public Date getEpcs_create_time() {
		return epcs_create_time;
	}
	
	/**
	 * 设置 信息添加时间
	 * @param epcs_create_time
	 */
	public void setEpcs_create_time(Date epcs_create_time) {
		this.epcs_create_time = epcs_create_time;
	}
	
	/**
	 * 获取 最后一次更新时间
	 * @return epcs_update_time
	 */
	public Date getEpcs_update_time() {
		return epcs_update_time;
	}
	
	/**
	 * 设置 最后一次更新时间
	 * @param epcs_update_time
	 */
	public void setEpcs_update_time(Date epcs_update_time) {
		this.epcs_update_time = epcs_update_time;
	}

	/**
	 * 获取 好评标识描述
	 * @return evaluation
	 */
	public String getEvaluation() {
		switch (epcs_evaluation) {
			case 0:
				evaluation = "好评";
				break;
			case 1:
				evaluation = "一般";
				break;
			case 2:
				evaluation = "差评";
				break;
			default:
				evaluation = "好评";
				break;
		}
		return evaluation;
	}
	

	/**
	 * 设置 好评标识描述
	 * @param evaluation
	 */
	public void setEvaluation(String evaluation) {
		this.evaluation = evaluation;
	}
	
	
}
