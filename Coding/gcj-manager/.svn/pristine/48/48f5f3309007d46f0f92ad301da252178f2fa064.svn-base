package com.yd.gcj.entity;

public class YdMangerFilesTask {

	/**任务id*/
	private Integer filetr_tid;
	/**文件信息id*/
	private Integer filetr_id;
	/**所属文件夹id*/
	private Integer filetr_fid;
	/**
	 * 获取 任务id
	 * @return filetr_tid
	 */
	public Integer getFiletr_tid() {
		return filetr_tid;
	}
	
	/**
	 * 设置 任务id
	 * @param filetr_tid
	 */
	public void setFiletr_tid(Integer filetr_tid) {
		this.filetr_tid = filetr_tid;
	}
	
	/**
	 * 获取 文件信息id
	 * @return filetr_id
	 */
	public Integer getFiletr_id() {
		return filetr_id;
	}
	
	/**
	 * 设置 文件信息id
	 * @param filetr_id
	 */
	public void setFiletr_id(Integer filetr_id) {
		this.filetr_id = filetr_id;
	}

	/**
	 * 获取 所属文件夹id
	 * @return filetr_fid
	 */
	public Integer getFiletr_fid() {
		return filetr_fid;
	}
	
	/**
	 * 设置 所属文件夹id
	 * @param filetr_fid
	 */
	public void setFiletr_fid(Integer filetr_fid) {
		this.filetr_fid = filetr_fid;
	}
	
}
