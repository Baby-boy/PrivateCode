    <script type="text/javascript">
    	$(function(){
    		$(".logo,.nav-logo").click(function(){
    			window.location.href="/index";
    		});
    		
    		var nvaNum = localStorage.getItem('nvaNum');
    		$(".nav-main-links li[class='active']").removeClass("active");
    		if(nvaNum!=0){
    			$(".nav-main-links li[data="+nvaNum+"]").addClass("active");
    		}
    	});
    	
    	
    	//头像闪动
    	setInterval(function(){
    		$('.feedback.user-portrait .pos-relative.pos-header').fadeToggle();
    	},100);
    	
    </script>

