package com.yd.gcj.entity;

public class YdMangerLabel {

	/**编号（主键）*/
	private Integer label_id;
	/**标签类型*/
	private Integer label_type;
	/**标签名称*/
	private String label_name;
	/**所属父级标签id*/
	private Integer label_pid;
	
	/**标识任务是否使用该标签（0未使用，1使用）*/
	private Integer is_select = 0;
	
	
	/**
	 * 获取 编号（主键）
	 * @return label_id
	 */
	public Integer getLabel_id() {
		return label_id;
	}
	
	/**
	 * 设置 编号（主键）
	 * @param label_id
	 */
	public void setLabel_id(Integer label_id) {
		this.label_id = label_id;
	}
	
	/**
	 * 获取 标签类型
	 * @return label_type
	 */
	public Integer getLabel_type() {
		return label_type;
	}
	
	/**
	 * 设置 标签类型
	 * @param label_type
	 */
	public void setLabel_type(Integer label_type) {
		this.label_type = label_type;
	}
	
	/**
	 * 获取 标签名称
	 * @return label_name
	 */
	public String getLabel_name() {
		return label_name;
	}
	
	/**
	 * 设置 标签名称
	 * @param label_name
	 */
	public void setLabel_name(String label_name) {
		this.label_name = label_name;
	}
	
	/**
	 * 获取 所属父级标签id
	 * @return label_pid
	 */
	public Integer getLabel_pid() {
		return label_pid;
	}
	

	/**
	 * 设置 所属父级标签id
	 * @param label_pid
	 */
	public void setLabel_pid(Integer label_pid) {
		this.label_pid = label_pid;
	}

	/**
	 * 获取 标识任务是否使用该标签（0未使用，1使用）
	 * @return is_select
	 */
	public Integer getIs_select() {
		return is_select;
	}
	
	/**
	 * 设置 标识任务是否使用该标签（0未使用，1使用）
	 * @param is_select
	 */
	public void setIs_select(Integer is_select) {
		this.is_select = is_select;
	}
	
}
