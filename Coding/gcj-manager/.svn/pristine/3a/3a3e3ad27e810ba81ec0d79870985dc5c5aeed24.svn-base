package com.yd.gcj.entity;

import java.util.Date;

public class YdMangerSpcs {

	/**编号（主键）*/
	private Integer spcs_id;
	/**服务商id*/
	private Integer spcs_sid;
	/**雇主（用户）id*/
	private Integer spcs_eid;
	/**任务id*/
	private Integer spcs_tid;
	/**任务名称*/
	private String spcs_tname;
	/**工期*/
	private String spcs_term;
	/**佣金*/
	private float spcs_price = 0;
	/**已支付佣金*/
	private float spcs_pay_price = 0;
	/**评价用户名称*/
	private String spcs_uname;
	/**评价等级 1:好;2:中;3:差*/
	private Integer spcs_grade;
	/**差评原因*/
	private String spcs_reason;
	/**分数*/
	private Integer spcs_score;
	/**内容*/
	private String spcs_content;
	/**信息创建时间*/
	private Date spcs_create_time;
	/**信息更新时间*/
	private Date spcs_update_time;
	
	/**评价等级描述*/
	private String grade = "";
	
	/**
	 * 获取 编号（主键）
	 * @return spcs_id
	 */
	public Integer getSpcs_id() {
		return spcs_id;
	}
	
	/**
	 * 设置 编号（主键）
	 * @param spcs_id
	 */
	public void setSpcs_id(Integer spcs_id) {
		this.spcs_id = spcs_id;
	}
	
	/**
	 * 获取 服务商id
	 * @return spcs_sid
	 */
	public Integer getSpcs_sid() {
		return spcs_sid;
	}
	
	/**
	 * 设置 服务商id
	 * @param spcs_sid
	 */
	public void setSpcs_sid(Integer spcs_sid) {
		this.spcs_sid = spcs_sid;
	}
	
	/**
	 * 获取 雇主（用户）id
	 * @return spcs_eid
	 */
	public Integer getSpcs_eid() {
		return spcs_eid;
	}
	
	/**
	 * 设置 雇主（用户）id
	 * @param spcs_eid
	 */
	public void setSpcs_eid(Integer spcs_eid) {
		this.spcs_eid = spcs_eid;
	}
	
	/**
	 * 获取 任务id
	 * @return spcs_tid
	 */
	public Integer getSpcs_tid() {
		return spcs_tid;
	}

	/**
	 * 设置 任务id
	 * @param spcs_tid
	 */
	public void setSpcs_tid(Integer spcs_tid) {
		this.spcs_tid = spcs_tid;
	}

	/**
	 * 获取 任务名称
	 * @return spcs_tname
	 */
	public String getSpcs_tname() {
		return spcs_tname;
	}

	/**
	 * 设置 任务名称
	 * @param spcs_tname
	 */
	public void setSpcs_tname(String spcs_tname) {
		this.spcs_tname = spcs_tname;
	}

	/**
	 * 获取 工期
	 * @return spcs_term
	 */
	public String getSpcs_term() {
		return spcs_term;
	}

	/**
	 * 设置 工期
	 * @param spcs_term
	 */
	public void setSpcs_term(String spcs_term) {
		this.spcs_term = spcs_term;
	}

	/**
	 * 获取 佣金
	 * @return spcs_price
	 */
	public float getSpcs_price() {
		return spcs_price;
	}

	/**
	 * 设置 佣金
	 * @param spcs_price
	 */
	public void setSpcs_price(float spcs_price) {
		this.spcs_price = spcs_price;
	}

	/**
	 * 获取 已支付佣金
	 * @return spcs_pay_price
	 */
	public float getSpcs_pay_price() {
		return spcs_pay_price;
	}

	/**
	 * 设置 已支付佣金
	 * @param spcs_pay_price
	 */
	public void setSpcs_pay_price(float spcs_pay_price) {
		this.spcs_pay_price = spcs_pay_price;
	}

	/**
	 * 获取 评价用户名称
	 * @return spcs_uname
	 */
	public String getSpcs_uname() {
		return spcs_uname;
	}
	
	/**
	 * 设置 评价用户名称
	 * @param spcs_uname
	 */
	public void setSpcs_uname(String spcs_uname) {
		this.spcs_uname = spcs_uname;
	}
	
	/**
	 * 获取 评价等级1:好;2:中;3:差
	 * @return spcs_grade
	 */
	public Integer getSpcs_grade() {
		return spcs_grade;
	}
	
	/**
	 * 设置 评价等级1:好;2:中;3:差
	 * @param spcs_grade
	 */
	public void setSpcs_grade(Integer spcs_grade) {
		this.spcs_grade = spcs_grade;
	}
	
	public String getSpcs_reason() {
		return spcs_reason;
	}

	public void setSpcs_reason(String spcs_reason) {
		this.spcs_reason = spcs_reason;
	}

	/**
	 * 获取 分数
	 * @return spcs_score
	 */
	public Integer getSpcs_score() {
		return spcs_score;
	}
	
	/**
	 * 设置 分数
	 * @param spcs_score
	 */
	public void setSpcs_score(Integer spcs_score) {
		this.spcs_score = spcs_score;
	}
	
	/**
	 * 获取 内容
	 * @return spcs_content
	 */
	public String getSpcs_content() {
		return spcs_content;
	}
	
	/**
	 * 设置 内容
	 * @param spcs_content
	 */
	public void setSpcs_content(String spcs_content) {
		this.spcs_content = spcs_content;
	}
	

	public Date getSpcs_create_time() {
		return spcs_create_time;
	}

	public void setSpcs_create_time(Date spcs_create_time) {
		this.spcs_create_time = spcs_create_time;
	}

	public Date getSpcs_update_time() {
		return spcs_update_time;
	}

	public void setSpcs_update_time(Date spcs_update_time) {
		this.spcs_update_time = spcs_update_time;
	}

	/**
	 * 获取 评价等级描述
	 * @return grade
	 */
	public String getGrade() {
		switch (spcs_grade) {
		case 0:
			grade = "好评";
			break;
		case 1:
			grade = "一般";	
			break;
		case 2:
			grade = "差评";
			break;
		default:
			grade = "好评";
			break;
		}
		return grade;
	}
	

	/**
	 * 设置 评价等级描述
	 * @param grade
	 */
	public void setGrade(String grade) {
		this.grade = grade;
	}
	
	
}
