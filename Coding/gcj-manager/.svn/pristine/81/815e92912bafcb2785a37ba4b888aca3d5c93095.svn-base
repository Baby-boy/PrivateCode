                                <div class="m_zlxg2">

                                    <ul></ul>

                                </div>
                                <div class="sele-tj"></div>
                            </div>

                            <input id="sfdq_num" type="hidden" value="" />

                            <input id="csdq_num" type="hidden" value="" />
                        	<!-- 省市值 -->
                            <input id="sfdq_tj" type="hidden" class="taskAddrP" value="北京" />
                        	<!-- 城市值 -->
                            <input id="csdq_tj" type="hidden" class="taskAddrC" value="市辖区" />
                        	<!-- 区域值 -->
